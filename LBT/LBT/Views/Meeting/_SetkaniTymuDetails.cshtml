@using LBT.ModelViews;
@using LBT.Resources
@model MeetingSetkaniTymuDetail

<section id="detailAdvancedForm">
    <table class="Main SetkaniTymuFormatting">
        <tbody>
            <tr>
                <td class="Box">
                    <table>
                        <tbody>
                            <tr>
                                <td>
                                    @Html.DisplayNameFor(m => m.MeetingTitleType):
                                    <b>@Html.DisplayFor(m => m.MeetingTitleType)</b>
                                </td>
                                <td>
                                    @Html.DisplayNameFor(m => m.DateStartTime):
                                    <b>@Html.DisplayFor(m => m.DateStartTime)</b>
                                </td>
                                <td colspan="2">
                                    @Html.DisplayNameFor(m => m.DateFinishTime):
                                    <b>@Html.DisplayFor(m => m.DateFinishTime)</b>
                                </td>
                            </tr>
                            <tr>
                                <td colspan="4">
                                    @Html.DisplayNameFor(m => m.Title):
                                    <b>@Html.DisplayFor(m => m.Title)</b>
                                </td>
                            </tr>
                            <tr>
                                <td colspan="2">
                                    @Html.DisplayNameFor(m => m.City):
                                    <b>@Html.DisplayFor(m => m.City)</b>
                                </td>
                                <td colspan="2">
                                    @Html.DisplayNameFor(m => m.MeetingKind):
                                    <b>@ListItemsResource.MeetingKind_Public</b>
                                </td>
                            </tr>
                            <tr>
                                <td colspan="2">
                                    @Html.DisplayNameFor(m => m.AddressLine1):
                                    <b>@Html.DisplayFor(m => m.AddressLine1)</b>
                                </td>
                                <td colspan="2">
                                    @Html.DisplayNameFor(m => m.Organizer):
                                    <b>@Html.DisplayFor(m => m.Organizer)</b>
                                </td>
                            </tr>
                            <tr>
                                <td>
                                    @Html.DisplayNameFor(m => m.FreeCapacity):
                                    <b>@Html.DisplayFor(m => m.FreeCapacity)</b>
                                </td>
                                <td>
                                    @Html.DisplayNameFor(m => m.Capacity):
                                    <b>@Html.DisplayFor(m => m.Capacity)</b>
                                </td>
                                <td colspan="2">
                                    @Html.DisplayNameFor(m => m.MainLeader):
                                    <b>@Html.DisplayFor(m => m.MainLeader)</b>
                                </td>
                            </tr>
                            <tr>
                                <td colspan="2">
                                    @Html.DisplayNameFor(m => m.RegisterDeadline):
                                    <b>@Html.DisplayFor(m => m.RegisterDeadline)</b>
                                </td>
                                <td colspan="2">
                                    @Html.DisplayNameFor(m => m.SecondaryLeader):
                                    <b>@Html.DisplayFor(m => m.SecondaryLeader)</b>
                                </td>
                            </tr>
                            <tr>
                                <td>
                                    @Html.DisplayNameFor(m => m.Price):
                                    <b>@Html.DisplayFor(m => m.Price)</b>
                                </td>
                                <td colspan="3">
                                    @Html.DisplayNameFor(m => m.AccountId):
                                    <b>@Html.DisplayFor(m => m.AccountId)</b>
                                </td>
                            </tr>
                            <tr>
                                <td>
                                    @Html.DisplayNameFor(m => m.SecondPrice):
                                    <b>@Html.DisplayFor(m => m.SecondPrice)</b>
                                </td>
                                <td colspan="3">
                                    @Html.DisplayNameFor(m => m.SecondAccountId):
                                    <b>@Html.DisplayFor(m => m.SecondAccountId)</b>
                                </td>
                            </tr>
                            <tr>
                                <td colspan="4">@ViewResource.Global_VariableSymbol_Text:
                                    <b>@ViewResource.Global_VariableSymbolDescription_Text</b> @ViewResource.Global_VariableSymbolExample_Text
                                </td>
                            </tr>
                            <tr>
                                <td colspan="4">
                                    @Html.DisplayNameFor(m => m.Ss):
                                    <b>@Html.DisplayFor(m => m.Ss)</b>
                                </td>
                            </tr>
                        </tbody>
                    </table>
                </td>
            </tr>
            <tr>
                <td class="Box BoxLongText">
                    <table>
                        <tbody>
                            <tr>
                                <td>
                                    <b>@Html.DisplayNameFor(m => m.Lecturer):</b>
                                    @Html.DisplayFor(m => m.Lecturer)
                                </td>
                            </tr>
                            <tr>
                                <td>
                                    <b>@Html.DisplayNameFor(m => m.BringYourOwn):</b>
                                    <div class="rawHtml">@Html.Raw(Model.BringYourOwn)</div>
                                </td>
                            </tr>
                            <tr>
                                <td>
                                    <b>@Html.DisplayNameFor(m => m.Note):</b>
                                    <div class="rawHtml">@Html.Raw(Model.Note)</div>
                                </td>
                            </tr>
                        </tbody>
                    </table>
                </td>
            </tr>
        </tbody>
    </table>
</section>
